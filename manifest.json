{
  "manifest_version": 3,
  "name": "SydneySweeneySwapper",
  "version": "1.0",
  "description": "",
  "author": "Harshendram",
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "*://twitter.com/*",
    "*://x.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://twitter.com/*",
        "*://x.com/*"
      ],
      "js": ["dist/mainscript.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["models/*", "target.webp", "alakh1.jpg", "alakh2.jpg", "alakh3.jpg","alakh4.jpg","alakh5.jpg"],
      "matches": [
        "*://twitter.com/*",
        "*://x.com/*"
      ]
    }
  ]
}
